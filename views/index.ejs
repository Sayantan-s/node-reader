<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/users.css">
</head>
<body>
    <h1>
        <%= header %>
    </h1>
    <div>
        <% data.forEach(({name,college,passOut,_id}) => {%>
            <figure class="user">
                <a href="/users/<%= _id %>">
                    <h2>
                        <%= name %>
                    </h2>
                </a>
                <h3>
                    <%= college %>
                </h3>
            </figure>
        <% }) %>
    </div>
</body>
</html>